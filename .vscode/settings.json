{
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "cSpell.words": [
    "ADDNUM",
    "ADDSYSTEM",
    "AGRC",
    "atch",
    "backoff",
    "Buildx",
    "CAROOT",
    "choco",
    "cloudrun",
    "codecov",
    "dotenv",
    "forcelist",
    "FULLADD",
    "geocoder",
    "geosearch",
    "Geospatial",
    "getenv",
    "gnis",
    "GUNNISON",
    "Holladay",
    "ilike",
    "instafail",
    "isort",
    "jsonify",
    "jsonpify",
    "Lehi",
    "mkcert",
    "nort",
    "oauth2accesstoken",
    "opensgid",
    "psycopg",
    "pylint",
    "pytest",
    "Referer",
    "simplejson",
    "STREETNAME",
    "UGRC",
    "UICLSID",
    "usaddress",
    "venv",
    "ython"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.rulers": [
    120
  ],
  "files.insertFinalNewline": true,
  "python.testing.pytestArgs": [
    "--no-cov"
  ],
  "python.testing.pytestEnabled": true,
}
